	-ansi|-std=c89|-std=iso9899:1990)
#! /bin/sh
# Call the appropriate C compiler with options to accept ANSI/ISO C
    case "$i" in
	    echo >&2 "`basename $0` called with non ANSI/ISO C option $i"
    esac
exec gcc $extra_flag ${1+"$@"}
	    exit 1
	    extra_flag=
extra_flag=-std=c89
for i; do
	-std=*)
