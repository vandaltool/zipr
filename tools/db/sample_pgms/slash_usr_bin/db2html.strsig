#! /bin/sh
  case $arg in
    continue
	echo "output is $output"
for arg in $*
  -h|--help|-v|--version)	break
if [ -d ${output} ]
  if [ $skip -gt 0 ]
jw -f docbook -b html -o ${output} "$@"
mkdir ${output}
  mv ${output} ${output}.junk
  -n|--nostd|-u|--nochunks)	;;
output=docbook2html-dir
  *)	output="$(echo $arg | sed 's,\.sgml$,,;s,\.sgm$,,;s,\.xml$,,')"
  rm -rf ${output}.junk
  -*)				skip=1
    skip=$(($skip - 1))
