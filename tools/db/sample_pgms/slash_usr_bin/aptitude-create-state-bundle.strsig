    for x in ${INPUTS[@]}; do echo $x; done
      --help )
   [ $HELP = 1 ]
	  HELP=1
if [ $FORCE_BZIP2 = 1 ] || ([ $FORCE_GZIP = 0 ] && which bzip2 2> /dev/null > /dev/null)
if ([ $PRINT_INPUTS = 0 ] && [ "$#" -ne 1 ]) ||
if [ $PRINT_INPUTS = 1 ]
  i=$((i + 1))
INPUTS[1]="$HOME/.aptitude"
